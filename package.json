    "webpack": "^1.12.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack -w -d",
    "build-js": "babel index.jsx --stage 0 --out-dir=dist --modules umd --module-id Rating",
